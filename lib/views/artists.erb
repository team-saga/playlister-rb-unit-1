<!DOCTYPE html>
<html>
<head>
  <title></title>
</head>
<body>
<table>
  <caption>Artists</caption>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
  <% @artist.songs each do |song| %>
    <tr>
      <td><%= link_to song.name song_path(@artist) %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<ul>
  <% @songs.each do |song| %>
   <li><a href="songs/<%= song.url%>.html" title=""><%= song.name %></a></li>
   <% end %>
 </ul>
</body>
</html>